{
    "name": "discord-xbot",
    "version": "1.0.0",
    "description": "A Discord Bot",
    "main": "app.js",
    "scripts": {
        "start": "node build/app.js",
        "dev": "nodemon src/app.ts",
        "devWindows": "nodemon --exec npx ts-node src/app.ts",
        "build": "rm -rf build/ && prettier --write src/ && tsc",
        "prepare": "husky install"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Wolemercy/discord-xbot.git"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/Wolemercy/discord-xbot/issues"
    },
    "homepage": "https://github.com/Wolemercy/discord-xbot#readme",
    "dependencies": {
        "@discordjs/builders": "^0.11.0",
        "@discordjs/rest": "^0.2.0-canary.0",
        "@prisma/client": "^3.7.0",
        "@quixo3/prisma-session-store": "^3.1.2",
        "axios": "^0.24.0",
        "body-parser": "^1.19.1",
        "cookie-parser": "^1.4.6",
        "crypto-js": "^4.1.1",
        "discord-api-types": "^0.26.1",
        "discord.js": "^13.5.0",
        "dotenv": "^10.0.0",
        "express": "^4.17.2",
        "express-session": "^1.17.2",
        "redis": "^4.0.1",
        "tedis": "^0.1.12",
        "winston": "^3.3.3"
    },
    "devDependencies": {
        "@types/body-parser": "^1.19.2",
        "@types/cookie-parser": "^1.4.2",
        "@types/crypto-js": "^4.1.0",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.13",
        "@types/express-session": "^1.17.4",
        "@types/node": "^17.0.8",
        "husky": "^7.0.4",
        "lint-staged": "^12.1.5",
        "nodemon": "^2.0.15",
        "prettier": "2.5.1",
        "prisma": "^3.7.0",
        "ts-node": "^10.4.0",
        "typescript": "^4.5.4"
    },
    "lint-staged": {
        "**/*": "prettier --write --ignore-unknown"
    },
    "prisma": {
        "seed": "ts-node src/db/seeds/seed.ts",
        "schema": "src/db/schema.prisma"
    }
}
